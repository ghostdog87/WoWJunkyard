<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"] - WoWJunkyard</title>
    <meta charset="utf-8" />
    <meta charset="UTF-8">
    <meta name="description" content="Game Warrior Template">
    <meta name="keywords" content="warrior, game, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Favicon -->
    <link href="img/favicon.ico" rel="shortcut icon" />

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,400i,500,500i,700,700i" rel="stylesheet">

    <!-- Stylesheets -->

    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/css/owl.carousel.css" />
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="~/css/animate.css" />
    <link rel="stylesheet" href="~/css/mainStyles-b59df8.css" />
    <script src="~/js/jquery-3.2.1.min.js"></script>
    


    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="mainBackgroundImage">
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>
    <!-- Header section -->
    <header class="header-section">
        <div class="container">
            <!-- logo -->
            <a class="site-logo" asp-area="" asp-controller="Home" asp-action="Index">
                <img src="~/img/LogoMakr_26SJod_v2.png" alt="">
            </a>
            <partial name="_LoginPartial" />
            <!-- responsive -->
            <div class="nav-switch">
                <i class="fa fa-bars"></i>
            </div>
            <!-- site menu -->
            <nav class="main-menu">
                <ul>
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-area="" asp-controller="News" asp-action="Index">News</a></li>
                    <li><a asp-area="" asp-controller="Character" asp-action="CharactersList">Characters</a></li>
                    <li><a asp-area="" asp-controller="WoWHeadNews" asp-action="News">WoWHead News</a></li>
                </ul>
            </nav>
        </div>
    </header>
<!-- Header section end -->
    <div class="row" style="margin-bottom: 50px;">
        <div class="col-sm-3"></div>
        <div class="col-sm-6">
            <div class="container">
                <partial name="_CookieConsentPartial" />
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>
        <div class="col-sm-3">
        </div>
    </div>


    <!-- Footer section -->
    <footer class="footer-section">
        <div class="container">
            <ul class="footer-menu">
                <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a asp-area="" asp-controller="News" asp-action="Index">News</a></li>
                <li><a asp-area="" asp-controller="WoWHeadNews" asp-action="News">WoWHead News</a></li>
            </ul>
        </div>
    </footer>
    <!-- Footer section end -->
    <!--====== Javascripts & Jquery ======-->

    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/jquery.marquee.min.js"></script>
    <script src="~/js/main.js"></script>
    <script src="https://cdn.tiny.cloud/1/71hdvdawo7jx50fokqhjesu454gu1kljtx5lj2ucpismfkps/tinymce/5/tinymce.min.js"></script>


@RenderSection("Scripts", required: false)
</body> 
</html>
